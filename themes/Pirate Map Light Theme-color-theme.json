{
	"name": "Pirate Map Light Theme",
	"tokenColors": [
		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other"
			],
			"settings": {
				"foreground": "#3d4a4e"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7e7a67"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#1c7477"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#915b15"
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#8d5bdd"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"support",
			],
			"settings": {
				"foreground": "#83122d"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.definition",
			],
			"settings": {
				"foreground": "#8d3045",
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"entity.name.scope-resolution",
				"entity.name.class",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#2173a3"
			}
		},
		{
			"name": "Namespace",
			"scope": [
				"entity.name.namespace",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#175072"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#3d4a4e",
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				// "foreground": "#615e5e",
				"foreground": "#5f4b2e",
			}
		},
		{
			"name": "keyword type",
			"scope": [
				"support.type",
				"support.constant",
			],
			"settings": {
				// "foreground": "#1c7477"
				"foreground": "#3d7d56"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#DC322F",
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "TypeScript/JavaScript Class",
			"scope": [
				"entity.name.type.class.ts",
				"entity.name.type.class.tsx",
				"entity.name.type.class.js",
				"entity.name.type.class.jsx"
			],
			"settings": {
				"foreground": "#2173a3"
			}
		},
		{
			"name": "TypeScript/JavaScript Interface",
			"scope": [
				"entity.name.type.interface.ts",
				"entity.name.type.interface.tsx"
			],
			"settings": {
				"foreground": "#2173a3"
			}
		},
		{
			"name": "TypeScript/JavaScript Module",
			"scope": [
				"entity.name.type.module.ts",
				"entity.name.type.module.tsx",
				"entity.name.type.module.js",
				"entity.name.type.module.jsx"
			],
			"settings": {
				"foreground": "#175072"
			}
		},
		{
			"name": "TypeScript/JavaScript Built-in Types",
			"scope": [
				"support.type.builtin.ts",
				"support.type.builtin.tsx",
				"support.type.primitive.ts",
				"support.type.primitive.tsx"
			],
			"settings": {
				"foreground": "#3d7d56"
			}
		},
		{
			"name": "TypeScript/JavaScript Decorators",
			"scope": [
				"meta.decorator.ts",
				"meta.decorator.tsx",
				"meta.decorator.js",
				"meta.decorator.jsx"
			],
			"settings": {
				"foreground": "#83122d"
			}
		}
	],
