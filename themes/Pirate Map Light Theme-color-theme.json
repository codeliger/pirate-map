{
	"name": "Pirate Map Light Theme",
	"tokenColors": [
		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other"
			],
			"settings": {
				"foreground": "#3d4a4e"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#93A1A1"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#1c7477"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#915b15"
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#943c4f"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"support",
			],
			"settings": {
				"foreground": "#a5445b"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.definition",
			],
			"settings": {
				"foreground": "#a1485D",
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"entity.name.scope-resolution"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#7090a3"
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"entity.name.scope-resolution"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#357ba3"
			}
		},
		{
			"name": "Namespace",
			"scope": [
				"entity.name.namespace",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#6b7980"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#3d4a4e",
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#727171",
			}
		},
		{
			"name": "keyword type",
			"scope": [
				"support.type",
			],
			"settings": {
				"foreground": "#5d8535"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#DC322F",
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
	],
